{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "secretsmanager:GetSecretValue"
            ],
            "Resource": [
                "arn:aws:secretsmanager:eu-north-1:913524923156:secret:pathfinders_db-*",
                "arn:aws:secretsmanager:eu-north-1:913524923156:secret:Pathfinders_GitHub_Access-*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:eu-north-1:913524923156:log-group:/aws/codebuild/pathfinders-build:*",
                "arn:aws:logs:eu-north-1:913524923156:log-group:/aws/codebuild/pathfinders-build:log-stream:*"
            ]
        }
    ]
} 